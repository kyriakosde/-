                                    <![CDATA[class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        #put initialization code here
        pass

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self):
        #access robot to motion capabilities
        motionProxy = ALProxy ("ALMotion")
        postureProxy = ALProxy ("ALRobotPosture")

        #initialize the footStep list
        footStepsList = []

        #set the footStep for each time constant
        #1
        footStepsList.append([["LLeg"], [[0.06,0.1,1.0]]])
        #2
        footStepsList.append([["LLeg"], [[0.0,0.16,0.0]]])
        #3
        footStepsList.append([["RLeg"], [[0.00,-0.1,0.0]]])
        #4
        footStepsList.append([["LLeg"], [[0.0,0.16,0.0]]])
        #5
        footStepsList.append([["RLeg"], [[-0.04,-0.1,0.0]]])
        #6
        footStepsList.append([["RLeg"], [[0.0,-0.16,0.0]]])
        #7
        footStepsList.append([["LLeg"], [[0.0,0.1,0.0]]])
        #8
        footStepsList.append([["RLeg"], [[0.0,-0.16,0.0]]])

        #Determine the frequency for each step
        stepFrequency = 0.8
        clearExisting = False
        #Repeat the sequence for:
        nbStepDance = 4

        for j in range( nbStepDance ):
            for i in range ( len(footStepsList) ):
                motionProxy.setFootStepsWithSpeed(footStepsList[i][0],footStepsList[i][1],           [stepFrequency],clearExisting)


        self.onStopped()


    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]>
