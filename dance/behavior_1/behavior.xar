                                    <![CDATA[import time
class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        footSteps = []
        motionProxy = ALProxy ("ALMotion")

        pnames="Body"
        pStiffnessLists = 1.0
        pTimeLists = 1.0
        motionProxy.stiffnessInterpolation(pnames,pStiffnessLists,pTimeLists)
    def onLoad(self):
        #put initialization code here
        pass

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self):
        #access robot to motion capabilities
        motionProxy = ALProxy ("ALMotion")
        postureProxy = ALProxy ("ALRobotPosture")

        #send NAO to Pose Init
        postureProxy.goToPosture("StandInit", 0.5)

        #a small step forwards and anti-clockwise with the left foot
        legName = ["LLeg"]
        x=0.2
        y=0.1
        Theta=0.0
        footSteps = [[x,y,Theta]]
        fractionMaxSpeed = [0.6]
        clearExisting = False

        motionProxy.setFootStepsWithSpeed(legName,footSteps,fractionMaxSpeed,clearExisting)

        # Wait for 5 seconds
        time.sleep(5.0)
        legName = ["RLeg"]
        x=0.2
        y=0.1
        Theta=0
        footSteps = [[x,y,Theta]]
        fractionMaxSpeed = [0.6]
        clearExisting = False


        motionProxy.setFootStepsWithSpeed(legName,footSteps,fractionMaxSpeed,clearExisting)
        time.sleep(5.0)
        self.onStopped() #activate the output of the box


    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]>
